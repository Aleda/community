<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- mapping文件用来配置entity和table -->
    <class name="cn.aleda.hibernate.entity.Photo" table="t_photos" catalog="community">
    	<id name="id" column="id">
    		<generator class="increment"></generator>
    	</id>
    	<property name="user_name" column="user_name"></property>
    	<property name="small_path" column="small_path"></property>
    	<property name="photo_name" column="photo_name"></property>
    	<property name="create_time" column="create_time"></property>
    	<property name="file_name" column="file_name"></property>
    	<property name="big_path" column="big_path"></property>
    </class>
</hibernate-mapping>
